# _*_ Makefile _*_

# target: dependencies
# 	action

# CC will be the compiler to be used
CC=gcc

# SRC will be the source cde files to be used
SRC=main.c school.c

all: school

school: main.o school.o
	$(CC) main.o school.o -o school

main.o: main.c m.h
	$(CC) -c $(SRC)

school.o: school.c m.h
	$(CC) -c $(SRC)

clean:
	rm -rf *.o school

.PHONY: clean
